        "/api/v1/user/edit": {
            "put": {
                "description": "Update user profile",
                "operationId": "fcb74221892fb8ca9a611d800c1df663",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "first_name",
                                    "last_name",
                                    "avatar",
                                    "is_marketing",
                                    "address"
                                ],
                                "properties": {
                                    "first_name": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "admin@email.com"
                                    },
                                    "last_name": {
                                        "type": "string",
                                        "format": "string",

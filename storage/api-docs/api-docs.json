{
    "openapi": "3.0.0",
    "info": {
        "title": "Authentication API",
        "version": "1.0"
    },
    "paths": {
        "/api/v1/admin/create": {
            "post": {
                "description": "Create a new user",
                "operationId": "80e784c531999530d7556054daf696ea",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "first_name",
                                    "last_name",
                                    "email",
                                    "phone_number",
                                    "password",
                                    "password_confirmation"
                                ],
                                "properties": {
                                    "first_name": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Mr."
                                    },
                                    "last_name": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Admin"
                                    },
                                    "email": {
                                        "type": "email",
                                        "format": "string",
                                        "example": "user@email.com"
                                    },
                                    "phone_number": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "+880175...."
                                    },
                                    "password": {
                                        "type": "password",
                                        "format": "string",
                                        "example": "*****"
                                    },
                                    "password_confirmation": {
                                        "type": "password",
                                        "format": "string",
                                        "example": "*****"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "User created successfully"
                    },
                    "400": {
                        "description": "Something went wrong"
                    }
                }
            }
        },
        "/api/v1/admin/user-listing": {
            "get": {
                "description": "User list",
                "operationId": "69df6167925692fe8f3d019cbc02256d",
                "parameters": [
                    {
                        "name": "page",
                        "in": "query",
                        "description": "No. of page",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "limit",
                        "in": "query",
                        "description": "How many record you want to get",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "first_name",
                        "in": "query",
                        "description": "First name of user",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "email",
                        "in": "query",
                        "description": "User email",
                        "required": false,
                        "schema": {
                            "type": "email"
                        }
                    },
                    {
                        "name": "phone_number",
                        "in": "query",
                        "description": "User phone",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "address",
                        "in": "query",
                        "description": "User address",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "created_at",
                        "in": "query",
                        "description": "Create time",
                        "required": false,
                        "schema": {
                            "type": "datetime"
                        }
                    },
                    {
                        "name": "marketing",
                        "in": "query",
                        "description": "Is marketing or not",
                        "required": false,
                        "schema": {
                            "type": "boolean"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "User list"
                    },
                    "400": {
                        "description": "Something went wrong"
                    }
                }
            }
        },
